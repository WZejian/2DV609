<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.ToggleButton?>
<?import javafx.scene.control.ToggleGroup?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="wholePage" prefHeight="688.0" prefWidth="718.0" styleClass="background" stylesheets="@FriendsPage_css.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox>
         <children>
            <HBox alignment="CENTER" spacing="20.0">
               <children>
                  <ToggleButton fx:id="unfinishedBtn" mnemonicParsing="false" onAction="#unfinishedBtnClick" selected="true" styleClass="postTypeBtn" stylesheets="@ProfileOverview_css.css" text="Unfinished">
                     <font>
                        <Font size="17.0" />
                     </font>
                     <toggleGroup>
                        <ToggleGroup fx:id="friendsTypeSwitch" />
                     </toggleGroup>
                  </ToggleButton>
                  <ToggleButton fx:id="finishedBtn" mnemonicParsing="false" onAction="#finishedBtnClick" styleClass="postTypeBtn" stylesheets="@ProfileOverview_css.css" text="Finished" toggleGroup="$friendsTypeSwitch">
                     <font>
                        <Font size="17.0" />
                     </font>
                  </ToggleButton>
               </children>
               <VBox.margin>
                  <Insets />
               </VBox.margin>
            </HBox>
            <VBox fx:id="resultPane" alignment="CENTER" prefHeight="200.0" prefWidth="100.0">
               <VBox.margin>
                  <Insets top="20.0" />
               </VBox.margin>
            </VBox>
         </children>
         <VBox.margin>
            <Insets bottom="30.0" left="20.0" right="20.0" top="30.0" />
         </VBox.margin>
      </VBox>
   </children>
</VBox>
